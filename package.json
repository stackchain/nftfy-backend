{
  "name": "nftfy-backend",
  "version": "1.0.0",
  "description": "Backend for Dapp",
  "main": "build/App.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "start": "tsc -w | firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "prettier:write": "./node_modules/prettier/bin-prettier.js --write ./src/**/*.{ts,tsx}",
    "prettier": "./node_modules/prettier/bin-prettier.js --check './src/**/*.{ts,tsx}'",
    "lint": "./node_modules/.bin/eslint src/**/*.{ts,tsx}",
    "firebase": "firebase",
    "serve": "./node_modules/nodemon/bin/nodemon.js src/Serve.ts"
  },
  "keywords": [],
  "author": "Nftfy Team",
  "license": "ISC",
  "dependencies": {
    "@sentry/node": "^5.30.0",
    "@sentry/tracing": "^5.30.0",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase-admin": "^9.4.2",
    "firebase-functions": "^3.13.0",
    "lodash": "^4.17.20",
    "web3": "^1.3.1",
    "web3-utils": "^1.3.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.20",
    "@types/web3": "^1.2.2",
